#coding=utf-8
import numpy as np
import xlrd
import xlwt
import re
m=re.compile(r'(\D+)(\d+)')
data=xlrd.open_workbook(r'C:\Users\sjzx\Desktop\多维聚类论文\就是这个数据！！！！.xlsx')
table=data.sheets()[0]
rowc=table.nrows
col=table.ncols
li=[]
arr=np.array(['ID','reason1','reason2','madicien','num'])
for i in range(10):
    if i==0:
        continue
    if i==2:
        for j in range(3,col):
            li=[]
            li.append(table.row_values(i)[0])
            li.append(table.row_values(i)[1])
            li.append(table.row_values(i)[2])
            value=table.row_values(i)[j]
            if value!='':
                ma=m.match(value)
                li.append(ma.group(1))
                li.append(float(ma.group(2)))
                arr=np.vstack((arr,np.array(li)))


f = xlwt.Workbook()
sheet1 = f.add_sheet(u'sheet1',cell_overwrite_ok=True) #创建sheet
for i in range(np.size(arr, 0)):
    for j in range(np.size(arr, 1)):
        sheet1.write(i,j,arr[i,j])
f.save(r'C:\Users\sjzx\Desktop\test.xls') #保存文件         
print(1)
